{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://localhost"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "https://github.com/yourusername/dozer/blob/main/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com/", "https://api.github.com/"],
      "headers": {
        "Accept": "application/vnd.github.v3+json"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206, 301, 302, 303, 307, 308]
}